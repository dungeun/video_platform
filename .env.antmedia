# Ant Media Server 환경 변수

# Ant Media Server URL
ANT_MEDIA_URL=https://stream.video.one-q.xyz:5443
ANT_MEDIA_HTTP_URL=http://stream.video.one-q.xyz:5080

# Ant Media Application
ANT_MEDIA_APP=LiveApp

# Ant Media REST API
ANT_MEDIA_REST_API=/rest/v2

# RTMP Settings
ANT_MEDIA_RTMP_URL=rtmp://stream.video.one-q.xyz/LiveApp

# WebRTC Settings
ANT_MEDIA_WEBSOCKET_URL=wss://stream.video.one-q.xyz:5443/LiveApp/websocket

# Vultr Object Storage (S3 Compatible)
VULTR_ACCESS_KEY=your-vultr-access-key
VULTR_SECRET_KEY=your-vultr-secret-key
VULTR_ENDPOINT=https://sgp1.vultrobjects.com
VULTR_REGION=sgp1
VULTR_BUCKET_NAME=videopick-recordings

# Recording Settings
RECORDING_ENABLED=true
RECORDING_MP4_ENABLED=true
RECORDING_ADD_TIMESTAMP=true

# Stream Settings
DEFAULT_STREAM_QUALITY=720p
ADAPTIVE_BITRATE_ENABLED=true
LOW_LATENCY_ENABLED=true

# Security
ANT_MEDIA_JWT_SECRET=your-jwt-secret-key
PUBLISH_TOKEN_REQUIRED=true

# Monitoring
HEALTH_CHECK_INTERVAL=60000
STATS_COLLECT_INTERVAL=5000